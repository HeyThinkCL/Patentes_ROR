<p id="notice"><%= notice %></p>

<h1>Api Locales</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_locales.each do |api_local| %>
      <tr>
        <td><%= link_to 'Show', api_local %></td>
        <td><%= link_to 'Edit', edit_api_local_path(api_local) %></td>
        <td><%= link_to 'Destroy', api_local, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Local', new_api_local_path %>

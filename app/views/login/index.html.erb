<body>
<div class="login">
  <div class="login-body">
    <a class="login-brand" href="index.html">
      <img class="img-responsive" src="http://heythink.cl/img/muni_nunoa.jpg" alt="Elephant">
    </a>
    <h3 class="login-heading">Iniciar Sesión</h3>
    <div class="login-form">
      <form data-toggle="md-validator"  data-toggle="md-validator" action="<%= url_for(controller: 'login', :action =>'new') %>" method="post">
        <div class="md-form-group md-label-floating">
          <input class="md-form-control" type="email" name="email" spellcheck="false" autocomplete="off" data-msg-required="Por favor ingrese un correo valido." required>
          <label class="md-control-label">E-mail</label>
        </div>
        <div class="md-form-group md-label-floating">
          <input class="md-form-control" type="password" name="password" minlength="6" data-msg-minlength="La contraseña debe tener al menos 6 caracteres." data-msg-required="Por favor ingrese su contraseña." required>
          <label class="md-control-label">Contraseña</label>
        </div>
        <div class="md-form-group md-custom-controls">
          <label class="custom-control custom-control-primary custom-checkbox">
            <input class="custom-control-input" type="checkbox" checked="checked">
            <span class="custom-control-indicator"></span>
            <span class="custom-control-label">Recordarme en este equipo</span>
          </label>
          <span aria-hidden="true"> · </span>
          <a href="password-3.html">¿Olvide mi Contraseña?</a>
        </div>
        <button class="btn btn-primary btn-block" type="submit">Entrar</button>
      </form>
    </div>
  </div>
  <div class="login-footer">
    Powered by heythink 2017
  </div>
</div>
<script src="js/vendor.min.js"></script>
